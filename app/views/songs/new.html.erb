<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %> 
  <br>
  <%= f.text_field :title %>
  <br>
  <br>
   <%= f.label :artist %> 
  <br>
  
  <%= f.text_field :artist_name %>
  <br>
  <br>
   <%= f.label :genre %> 
  <br>
  
  <%= f.collection_select :genre_id, Genre.all, :name, :name %>
  <br>
  <br>
  <textarea name="song[note_contents][]" id="song_notes_attributes_0_content"></textarea>
  <textarea name="song[note_contents][]" id="song_notes_attributes_1_content"></textarea>
  
   
  <br />


  <%= f.submit %>
<% end %>


